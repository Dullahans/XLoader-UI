{
  "version": "1.0",
  "description": "Bin文件参数字典 - 定义参数的标准命名、含义、范围和单位",
  "parameters": {
    "up_limit": {
      "name": "上限值",
      "description": "输出信号的上限阈值",
      "unit": "",
      "type": "number",
      "min": 0,
      "max": 65535,
      "default": 2200
    },
    "down_limit": {
      "name": "下限值",
      "description": "输出信号的下限阈值",
      "unit": "",
      "type": "number",
      "min": 0,
      "max": 65535,
      "default": 0
    },
    "vout_gain": {
      "name": "电压输出增益",
      "description": "输出电压的放大系数",
      "unit": "",
      "type": "number",
      "min": 0,
      "max": 10,
      "step": 0.1,
      "default": 1.0
    },
    "current_output_gain": {
      "name": "电流输出增益",
      "description": "输出电流的放大系数",
      "unit": "",
      "type": "number",
      "min": 0,
      "max": 10,
      "step": 0.1,
      "default": 1.0
    },
    "stuck_mode": {
      "name": "堵转模式",
      "description": "电机堵转时的保护阈值",
      "unit": "ms",
      "type": "number",
      "min": 0,
      "max": 10000,
      "default": 2000
    },
    "matrix1": {
      "name": "矩阵参数1",
      "description": "插值查表用的一维数组",
      "unit": "",
      "type": "array"
    },
    "signal_tailgate_1": {
      "name": "尾门信号1",
      "description": "尾门传感器1的使能状态",
      "unit": "",
      "type": "boolean",
      "options": [0, 1],
      "default": 1
    },
    "signal_tailgate_2": {
      "name": "尾门信号2",
      "description": "尾门传感器2的使能状态",
      "unit": "",
      "type": "boolean",
      "options": [0, 1],
      "default": 1
    },
    "example_factor": {
      "name": "示例系数表",
      "description": "二维插值查表数据，格式：[索引] [输入值] [输出值]",
      "unit": "",
      "type": "matrix",
      "columns": ["Index", "Input", "Output"]
    },
    "max_speed": {
      "name": "最大转速",
      "description": "电机允许的最大转速",
      "unit": "RPM",
      "type": "number",
      "min": 0,
      "max": 10000,
      "default": 3000
    },
    "min_speed": {
      "name": "最小转速",
      "description": "电机允许的最小转速",
      "unit": "RPM",
      "type": "number",
      "min": 0,
      "max": 10000,
      "default": 0
    },
    "acceleration": {
      "name": "加速度",
      "description": "电机加速时的变化率",
      "unit": "RPM/s",
      "type": "number",
      "min": 0,
      "max": 1000,
      "default": 100
    },
    "deceleration": {
      "name": "减速度",
      "description": "电机减速时的变化率",
      "unit": "RPM/s",
      "type": "number",
      "min": 0,
      "max": 1000,
      "default": 150
    },
    "pid_kp": {
      "name": "PID比例系数",
      "description": "PID控制器的比例增益Kp",
      "unit": "",
      "type": "number",
      "min": 0,
      "max": 100,
      "step": 0.01,
      "default": 1.0
    },
    "pid_ki": {
      "name": "PID积分系数",
      "description": "PID控制器的积分增益Ki",
      "unit": "",
      "type": "number",
      "min": 0,
      "max": 100,
      "step": 0.001,
      "default": 0.1
    },
    "pid_kd": {
      "name": "PID微分系数",
      "description": "PID控制器的微分增益Kd",
      "unit": "",
      "type": "number",
      "min": 0,
      "max": 100,
      "step": 0.001,
      "default": 0.01
    },
    "brake_force": {
      "name": "制动力",
      "description": "制动器施加的力度百分比",
      "unit": "%",
      "type": "number",
      "min": 0,
      "max": 100,
      "default": 80
    },
    "release_delay": {
      "name": "释放延迟",
      "description": "制动器释放前的等待时间",
      "unit": "ms",
      "type": "number",
      "min": 0,
      "max": 1000,
      "default": 50
    }
  },
  "modules": {
    "WINDOW_R": {
      "name": "右车窗",
      "description": "右侧车窗电机控制模块"
    },
    "WINDOW_L": {
      "name": "左车窗", 
      "description": "左侧车窗电机控制模块"
    },
    "TAIL_GATE_LOCK": {
      "name": "尾门锁",
      "description": "尾门锁控制模块"
    },
    "MOTOR_CTRL": {
      "name": "电机控制",
      "description": "通用电机控制模块"
    },
    "BRAKE_CTRL": {
      "name": "制动控制",
      "description": "制动系统控制模块"
    }
  }
}



